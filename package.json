{
  "name": "jikken",
  "displayName": "Jikken",
  "description": "Language support for Jikken Tests",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "./images/favicon.ico",
  "contributes": {
    "languages": [
      {
        "id": "jikken",
        "icon": {
          "light": "./images/favicon.ico",
          "dark": "./images/favicon.ico"
        },
        "aliases": [
          "Jikken Test",
          "jikken"
        ],
        "extensions": [
          ".jkt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jikken",
        "scopeName": "source.jkt",
        "path": "./syntaxes/jikken.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jikken",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}